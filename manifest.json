{
  "manifest_version": 3,
  "name": "Rawkuma Chapter Downloader",
  "version": "1.0.0",
  "permissions": ["activeTab", "tabs","downloads"],
  "host_permissions": ["<all_urls>"],
  "action": { "default_popup": "popup/popup.html" },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
